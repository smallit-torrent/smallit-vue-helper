  "Zip Code Input Snippet": {
    "prefix": [
      "zip-code-input",
      "input:zip-code"
    ],
    "body": [
      "<zip-code-input",
      "  v-model=\"\"",
      "  v-model:address=\"\"",
      "  clearable",
      "  @change=\"\"",
      "  @address-data-change=\"\"",
      ">",
      "  <template #prefix>",
      "    <el-icon><Location /></el-icon>",
